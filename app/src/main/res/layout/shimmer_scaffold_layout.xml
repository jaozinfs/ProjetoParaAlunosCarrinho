<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="8dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/user_icon"
            android:layout_width="150dp"
            android:layout_height="@dimen/product_item_image_height"
            android:layout_marginStart="8dp"
            android:background="#C2C2C2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/user_name"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/user_icon"
            app:layout_constraintTop_toBottomOf="@+id/user_icon" />

        <TextView
            android:id="@+id/user_status"
            android:layout_width="64dp"
            android:layout_height="wrap_content"
            android:background="#C2C2C2"
            android:textSize="12sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="@+id/user_icon"
            app:layout_constraintTop_toTopOf="@+id/user_name" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/user_icon"
            app:layout_constraintTop_toBottomOf="@+id/user_name" />


        <TextView
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/user_status"
            app:layout_constraintStart_toEndOf="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/user_name" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/constraintLayout3"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/user_icon1"
            android:layout_width="150dp"
            android:layout_height="@dimen/product_item_image_height"
            android:layout_marginStart="8dp"
            android:background="#C2C2C2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/user_name1"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/user_icon1"
            app:layout_constraintTop_toBottomOf="@+id/user_icon1" />

        <TextView
            android:id="@+id/user_status1"
            android:layout_width="64dp"
            android:layout_height="wrap_content"
            android:background="#C2C2C2"
            android:textSize="12sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="@+id/user_icon1"
            app:layout_constraintTop_toTopOf="@+id/user_name1" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/user_icon1"
            app:layout_constraintTop_toBottomOf="@+id/user_name1" />


        <TextView
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/user_status1"
            app:layout_constraintStart_toEndOf="@+id/textView1"
            app:layout_constraintTop_toBottomOf="@+id/user_name1" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

        <ImageView
            android:id="@+id/user_icon2"
            android:layout_width="150dp"
            android:layout_height="@dimen/product_item_image_height"
            android:layout_marginStart="8dp"
            android:background="#C2C2C2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/user_name2"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/user_icon2"
            app:layout_constraintTop_toBottomOf="@+id/user_icon2" />

        <TextView
            android:id="@+id/user_status2"
            android:layout_width="64dp"
            android:layout_height="wrap_content"
            android:background="#C2C2C2"
            android:textSize="12sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="@+id/user_icon2"
            app:layout_constraintTop_toTopOf="@+id/user_name2" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/user_icon2"
            app:layout_constraintTop_toBottomOf="@+id/user_name2" />


        <TextView
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/user_status2"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/user_name2" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/constraintLayout5"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

        <ImageView
            android:id="@+id/user_icon3"
            android:layout_width="150dp"
            android:layout_height="@dimen/product_item_image_height"
            android:layout_marginStart="8dp"
            android:background="#C2C2C2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/user_name3"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/user_icon3"
            app:layout_constraintTop_toBottomOf="@+id/user_icon3" />

        <TextView
            android:id="@+id/user_status3"
            android:layout_width="64dp"
            android:layout_height="wrap_content"
            android:background="#C2C2C2"
            android:textSize="12sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="@+id/user_icon3"
            app:layout_constraintTop_toTopOf="@+id/user_name3" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/user_icon3"
            app:layout_constraintTop_toBottomOf="@+id/user_name3" />


        <TextView
            android:layout_width="@dimen/cart_button_width"
            android:layout_height="@dimen/cart_button_height"
            android:layout_marginTop="10dp"
            android:background="#C2C2C2"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/user_status3"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/user_name3" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>